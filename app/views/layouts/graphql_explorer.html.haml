!!!
%html.layout-pf.layout-pf-fixed.transitions{:lang => I18n.locale.to_s.sub('-', '_')}
  %body
    %p
    %title ManageIQ GraphQL Explorer
    = stylesheet_link_tag "application", media: "all"
    = stylesheet_link_tag "manageiq/graphql/application", media: "all"
    = javascript_pack_tag 'vendor'
    = javascript_include_tag "application"
    = javascript_pack_tag 'manageiq-ui-classic/application-common'
    = javascript_pack_tag 'manageiq-graphql/graphql-explorer'
    = csrf_meta_tags
    = render :partial => "layouts/header"
    = render :partial => "layouts/vertical_navbar"
    .container-pf-nav-pf-vertical.container-pf-nav-pf-vertical-with-sub-menus{:style => "overflow: hidden; height: 100%"}
      = yield
